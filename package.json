{
  "name": "front-mobile-vue",
  "version": "1.0.0",
  "private": true,
  "homepage": "https://github.com/poohlaha/front-mobile-vue.git",
  "engines": {
    "node": ">=16.0.0"
  },
  "description": "`Vue` `手机` 端框架模块.",
  "keywords": [
    "vue",
    "css",
    "js"
  ],
  "author": "poohlaha",
  "license": "MIT/Apache-2.0",
  "scripts": {
    "start": "node ./scripts/build.js start",
    "dev": "rimraf build && node ./scripts/build.js build",
    "simulate": "rimraf build && node ./scripts/build.js simulate",
    "prod": "rimraf dist && node ./scripts/build.js prod",
    "format": "npx prettier --write src",
    "report": "jscpd --pattern 'src/views/**/*.{vue,ts,js}'",
    "updater": "node ./scripts/updater.js"
  },
  "dependencies": {
    "lib-flexible": "^0.3.2",
    "core-js": "^3.8.3",
    "crypto-js": "^4.1.1",
    "fastclick": "^1.0.6",
    "vant": "2.12.54",
    "axios": "^0.21.1"
  },
  "devDependencies": {
    "@bale-tools/mutate-service": "^2.2.4",
    "@bale-tools/mutate-version": "^2.1.6",
    "toml": "^3.0.0",
    "jscpd": "^3.4.5",
    "vconsole": "^3.9.5",
    "core-js": "^3.8.3",
    "husky": "^4.2.3",
    "prettier": "^2.4.1",
    "lint-staged": "^13.0.3",
    "babel-plugin-import": "^1.13.3",
    "@commitlint/cli": "^12.1.4",
    "compression-webpack-plugin": "^10.0.0",
    "workbox-webpack-plugin": "^6.5.4",
    "@vue/cli-plugin-babel": "~5.0.0",
    "babel-eslint": "^10.1.0",
    "@babel/eslint-parser": "^7.12.16",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-plugin-router": "~5.0.0",
    "@vue/eslint-config-standard": "^5.1.2",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.0.3",
    "vue-template-compiler": "^2.7.14",
    "n-readlines": "^1.0.1",
    "lib-flexible": "^0.3.2",
    "postcss-aspect-ratio-mini": "^1.1.0",
    "postcss-write-svg": "^3.0.1"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8",
    "iOS >= 8",
    "Firefox >= 20",
    "Android > 4.4"
  ],
   "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "**/*.{vue,jsx,js,tsx,ts,json,css,md}": [
      "prettier --write"
    ],
    "**/*.{vue,jsx,js,tsx,ts}": [
      "eslint --fix"
    ]
  }
}
